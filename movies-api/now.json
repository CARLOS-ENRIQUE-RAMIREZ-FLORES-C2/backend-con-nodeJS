{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "DB_USER": "db_user_node",
        "DB_PASSWORD": "db-password",
        "DB_HOST": "db-host",
        "DB_NAME": "db-name"
    }
}